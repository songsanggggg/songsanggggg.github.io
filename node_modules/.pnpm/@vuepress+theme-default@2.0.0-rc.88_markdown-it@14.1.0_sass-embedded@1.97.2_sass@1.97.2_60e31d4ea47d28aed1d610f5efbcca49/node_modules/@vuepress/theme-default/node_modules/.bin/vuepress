#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/songsanggggg/Desktop/BlogScript/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepress+bundler-vite@2.0.0-rc.20_@types+node@25.0.6_sass-embedded@1.97.2_sass@1.97.2__vue@3.5.26/node_modules/vuepress/bin/node_modules:/home/songsanggggg/Desktop/BlogScript/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepress+bundler-vite@2.0.0-rc.20_@types+node@25.0.6_sass-embedded@1.97.2_sass@1.97.2__vue@3.5.26/node_modules/vuepress/node_modules:/home/songsanggggg/Desktop/BlogScript/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepress+bundler-vite@2.0.0-rc.20_@types+node@25.0.6_sass-embedded@1.97.2_sass@1.97.2__vue@3.5.26/node_modules:/home/songsanggggg/Desktop/BlogScript/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/songsanggggg/Desktop/BlogScript/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepress+bundler-vite@2.0.0-rc.20_@types+node@25.0.6_sass-embedded@1.97.2_sass@1.97.2__vue@3.5.26/node_modules/vuepress/bin/node_modules:/home/songsanggggg/Desktop/BlogScript/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepress+bundler-vite@2.0.0-rc.20_@types+node@25.0.6_sass-embedded@1.97.2_sass@1.97.2__vue@3.5.26/node_modules/vuepress/node_modules:/home/songsanggggg/Desktop/BlogScript/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepress+bundler-vite@2.0.0-rc.20_@types+node@25.0.6_sass-embedded@1.97.2_sass@1.97.2__vue@3.5.26/node_modules:/home/songsanggggg/Desktop/BlogScript/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vuepress@2.0.0-rc.20_@vuepress+bundler-vite@2.0.0-rc.20_@types+node@25.0.6_sass-embedded@1.97.2_sass@1.97.2__vue@3.5.26/node_modules/vuepress/bin/vuepress.js" "$@"
else
  exec node  "$basedir/../../../../../../vuepress@2.0.0-rc.20_@vuepress+bundler-vite@2.0.0-rc.20_@types+node@25.0.6_sass-embedded@1.97.2_sass@1.97.2__vue@3.5.26/node_modules/vuepress/bin/vuepress.js" "$@"
fi
